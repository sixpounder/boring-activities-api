{
  "name": "boring_activities",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "deno fmt && deno task build:doc",
    "build:doc": "webpack build --config webpack.config.js --mode production",
    "build:doc:dev": "webpack-dev-server --config webpack.config.js --mode development",
    "build:analyze": "webpack build --config webpack.config.js --mode development && webpack-bundle-analyzer dist/public/stats.json",
    "dev": "deno run --allow-all ./node_modules/.bin/concurrently --kill-others \"deno --allow-all --watch src/index.ts\" \"deno task build:doc:dev\"",
    "start": "deno run build:doc && deno --allow-all src/index.ts"
  },
  "author": "Andrea",
  "license": "ISC",
  "description": "A sample rest API for todo apps",
  "dependencies": {
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^11.11.10",
    "helmet": "^8.0.0",
    "highlight.js": "^11.10.0",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.1",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.8",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
